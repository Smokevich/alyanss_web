{{ header }} 
{{ column_left }} 
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %} 
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %} 
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    <div class="panel panel-default"> 
      <div class="panel-body">
      <table class="table table-striped">
      <thead>
        <tr>
            <th>ID</th>
            <th>Номер заказа</th>
            <th>Сумма</th>
            <th>Пользователь</th>
            <th>email</th>
            <th>Дата создания</th>
            <th>Дата оплаты</th>
            <th>Идентификатор операции</th>
        </tr>
        </thead>
        <tbody>
         {% for viewstatus in viewstatuses %} 
          <tr>
          <td>{{ viewstatus.yandex_id}}</td>
          <td>{{ viewstatus.num_order}}</td>
          <td>{{ viewstatus.sum}}</td>
          <td>{{ viewstatus.user}}</td>
          <td>{{ viewstatus.email}}</td>
          <td>{{ viewstatus.date_created}}</td>
          <td>{{ viewstatus.date_enroled}}</td>
          <td>{{ viewstatus.sender}}</td>
          </tr>
        {% endfor %}
 
        </tbody>
        </table>

  </div>
</div>
</div>
</div>
{{ footer }} 
